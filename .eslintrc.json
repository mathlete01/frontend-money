{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-react": "off",   
        "react/jsx-uses-vars": "off",
         "no-unused-vars": "off",
         "react/prop-types": "off",
         "react/no-unescaped-entities" : "off"
    }
}
